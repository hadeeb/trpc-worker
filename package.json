{
	"name": "@hadeeb/trpc-worker",
	"version": "0.0.1",
	"description": "Run tRPC in a web worker",
	"type": "module",
	"sideEffects": false,
	"scripts": {
		"prepublishOnly": "npm run build",
		"build": "rollup -c"
	},
	"keywords": ["trpc", "worker", "web worker", "electron"],
	"author": "Hadeeb Farhan",
	"license": "MIT",
	"peerDependencies": {
		"@trpc/client": "^10.0.0",
		"@trpc/server": "^10.0.0"
	},
	"files": ["src", "dist"],
	"exports": {
		"./adapter": {
			"types": "./dist/adapter.d.ts",
			"import": "./dist/adapter.mjs",
			"require": "./dist/adapter.js"
		},
		"./link": {
			"types": "./dist/link.d.ts",
			"import": "./dist/link.mjs",
			"require": "./dist/link.js"
		}
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.6",
		"@trpc/client": "^10.45.2",
		"@trpc/server": "^10.45.2",
		"@types/node": "^16.18.96",
		"@types/ws": "^8.5.10",
		"electron": "^22.3.27",
		"rollup": "^4.16.1",
		"rollup-plugin-node-externals": "^7.1.1",
		"typescript": "^5.4.5"
	}
}
